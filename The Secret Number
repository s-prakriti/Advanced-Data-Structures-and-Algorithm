import sys

def main():
    l = 1
    r = 1000000
    while l < r:
        mid = (l + r + 1) // 2
        print(mid)
        sys.stdout.flush()  # Ensure the query is sent immediately
        response = input().strip()
        if response == "<":
            r = mid - 1
        else:
            l = mid
    print(f"! {l}")
    sys.stdout.flush()

if __name__ == "__main__":
    main()
